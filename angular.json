{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Pokemon": {
      "root": "",
      "sourceRoot": "docs",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Pokemon",
            "index": "docs/index.html",
            "main": "docs/main.ts",
            "polyfills": "docs/polyfills.ts",
            "tsConfig": "docs/tsconfig.app.json",
            "assets": [
              "docs/favicon.ico",
              "docs/assets"
            ],
            "styles": [
              "docs/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "docs/environments/environment.ts",
                "with": "docs/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Pokemon:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Pokemon:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Pokemon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "docs/test.ts",
            "polyfills": "docs/polyfills.ts",
            "tsConfig": "docs/tsconfig.spec.json",
            "karmaConfig": "docs/karma.conf.js",
            "styles": [
              "docs/styles.css"
            ],
            "scripts": [],
            "assets": [
              "docs/favicon.ico",
              "docs/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "docs/tsconfig.app.json",
              "docs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "Pokemon-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Pokemon:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Pokemon:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "Pokemon"
}
